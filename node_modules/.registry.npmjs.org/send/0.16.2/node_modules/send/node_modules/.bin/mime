#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/cli.js/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/node_modules:/home/philippe/Documents/git/multi_stream/node_modules:/home/philippe/Documents/git/node_modules:/home/philippe/Documents/node_modules:/home/philippe/node_modules:/home/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../mime/1.4.1/node_modules/mime/cli.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/cli.js/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules/mime/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/1.4.1/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/mime/node_modules:/home/philippe/Documents/git/multi_stream/node_modules/.registry.npmjs.org/node_modules:/home/philippe/Documents/git/multi_stream/node_modules:/home/philippe/Documents/git/node_modules:/home/philippe/Documents/node_modules:/home/philippe/node_modules:/home/node_modules:/node_modules" node  "$basedir/../../../../../../mime/1.4.1/node_modules/mime/cli.js" "$@"
  ret=$?
fi
exit $ret
